package proje3;

public class HeapNode {

	private milliPark iData; // data item (key)
	// -------------------------------------------------------------
	public HeapNode(milliPark key)
	{ iData = key; }
	// -------------------------------------------------------------
	public milliPark getKey()
	{ return iData; }
	// -------------------------------------------------------------
	public void setKey(milliPark id)
	{ iData = id; }
	
	public String toString() {
		return iData.toString();
 	}

}
